//reverse input buser se lekr

#include <iostream>
#include <cmath>


using namespace std;

int main()
{
    int input;
    cin >> input;

    int arr[input];
    for (int i = 0; i < input; i++)
    {
        cin >> arr[i];
        for (int i = input - 1; i >= 0; i--)
        {
            cout << arr[i] << " ";
        }
    }

    return 0;
}
