#include<iostream>

using namespace std;
int main (){ 
    int arr[]={3,4,6,7,1};
    int target=7;
    int pairs =0;
    for(int i=0;i<5;i++){                                 //pairs find kr rhe h sum =7
        for(int j=i+1;j<5;j++){
            if(arr[i]+arr[j]==target){
                pairs++;
            }
        }
    }
    cout<<pairs;
    return 0;
}
