//find sum of 10 natural number i array. in function


#include <iostream>
using namespace std;
int sum(int num){
    int sum=0;
    for(int i=0;i<=num;i++){
        sum +=i;
    }
    return sum;

}
int main(){
int num=10;
int res=sum(num);

cout<<res;
    return 0;
}
